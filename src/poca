import poco

def main():
    '''Main script'''
    try:
        args = poco.args.get_args()
        poco.plogging.get_logger(args)
        config = poco.config.Config(args)
        for sub in config.subs:
            channel = poco.channel.Channel(config, sub)
    except KeyboardInterrupt:
        pass

if __name__ == '__main__':
    main()
