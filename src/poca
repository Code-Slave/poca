import poco 


if __name__ == '__main__':
    try:
        args = poco.args.get_args()
        out = poco.output.Out(args)
        config = poco.config.Config(args, out)
        for sub in config.subs:
            channel = poco.channel.Channel(config, out, sub)
    except KeyboardInterrupt:
        pass


